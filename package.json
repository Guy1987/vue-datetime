{
  "name": "vue-datetime3",
  "version": "1.0.1",
  "description": "Mobile friendly datetime picker for Vue. Supports date, datetime and time modes, i18n and disabling dates.",
  "keywords": [
    "datetime",
    "datetime-picker",
    "picker",
    "date",
    "vue"
  ],
  "author": "Mario Ju√°rez <mario@mjp.one>",
  "maintainers": [
    "Velis <info@velis.si>"
  ],
  "main": "dist/vue-datetime.umd.js",
  "types": "dist/vue-datetime.d.ts",
  "module": "dist/vue-datetime.mjs",
  "style": "dist/style.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "demo": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly -p tsconfig.build.json",
    "clean": "rimraf dist && rimraf build/dist",
    "lint": "eslint src && vue-tsc --noEmit",
    "version": "auto-changelog -p --starting-commit 975d478b80b8e1ed3663f55b34c3a35521a43bdb --commit-limit false && git add CHANGELOG.md",
    "test": "echo \"Error: tests not working yet\""
  },
  "pre-commit": "--no-verify",
  "devDependencies": {
    "@types/luxon": "^3.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "auto-changelog": "^1.3.0",
    "clean-css": "^4.0.0",
    "eslint-config-velis": "^1.1.14",
    "gh-pages": "^1.1.0",
    "luxon": "^1.0.0",
    "np": "^7.6.3",
    "pre-commit": "^1.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0",
    "weekstart": "^1.0.0"
  },
  "peerDependencies": {
    "luxon": "^1.0.0",
    "vue": "^3.2.47",
    "weekstart": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariomka/vue-datetime.git"
  },
  "bugs": {
    "url": "https://github.com/mariomka/vue-datetime/issues"
  },
  "homepage": "https://github.com/mariomka/vue-datetime#readme",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "velis"
    ],
    "rules": {
      "vuejs-accessibility/click-events-have-key-events": "off",
      "vuejs-accessibility/form-control-has-label": "off",
      "vuejs-accessibility/label-has-for": "off"
    },
    "ignorePatterns": [
      "dist/*",
      "coverage/*",
      "node_modules/*",
      "build/*",
      "vite.config.ts"
    ]
  }
}
